<root>
	<styles>
		<include src="file://{resources}/styles/csgostyles.css" />
		<include src="file://{resources}/styles/hud/hud.css" />
	</styles>

	<CSGOHud hittest="false">
		                                                                           
		
		                                                                                     
		<Panel id="HudInWorld" hittest="false" hittestchildren="false">
			<CSGOHudDamageIndicator id="HudDamageIndicator"/>
			<CSGOHudReticle id="HudReticle"/>
		</Panel>

		                                                                            
		<Panel id="HudSpectatorVignetting" hittest="false" hittestchildren="false" />

		                                                               
		<Panel id="HudCenter" hittest="false" hittestchildren="false">
			<CSGOHudAlerts id="HudAlerts"/>
			<CSGOHudDemolitionProgression id="HudDemolitionProgression" />
		</Panel>

		                                                                   
		<CSGOHudSpecPlayer id="HudSpecPlayer" hittest="false" hittestchildren="false" />

		                                                    
		<CSGOHudWinPanel id="HudWinPanel" hittest="false" hittestchildren="false" />

		                                                                                     
		<Panel id="HudTopLeft" hittest="false" hittestchildren="false">
			<CSGOHudRadar id="HudRadar"/>
			<CSGOHudMoney id="HudMoney"/>
			<CSGOHudGameIcons id="HudGameIcons"/>
			<CSGOHudMissionPanel id="HudMissionPanel" />
			<CSGOHudGuardianPanel id="HudGuardianPanel" />
			<CSGOHudDMBonusPanel id="HudDMBonusPanel" />
			<Panel />                                                                        
		</Panel>

		                                                                            
		                                                                              
			                                                                            
			                                                                       
			                                        
			   
		<Panel id="HudTopCenter" hittest="false" hittestchildren="false">
		</Panel>

		                                                              
		<CSGOHudTeamCounter id="HudTeamCounter" hittest="false" hittestchildren="false" />

		                                                                                
		                                                                
		<Panel id="HudBottomCenter" hittest="false" hittestchildren="false">
			<CSGOHudProgressBar id="HudProgressBar"/>
			<CSGOHudHintText id="HudHintText"/>
		</Panel>

		                                                                                      
		<Panel id="HudLowerLeft" hittest="false" hittestchildren="false">
			<CSGOHudVoiceStatus id="Status"/>
			<CSGOHudHealthArmor id="HudHealthArmor"/>
		</Panel>

		                                                                
		<CSGOHudRadio id="HudRadio" hittest="false" hittestchildren="false" />

		                                                                           
		<Panel id="HudTopRight" hittest="false" hittestchildren="false">
			<CSGOHudAutoDisconnect id="HudAutoDisconnect"/>
			<CSGOHudDeathNotice id="HudDeathNotice"/>
		</Panel>

		                                                                   
		<Panel id="HudBottomRight" hittest="false" hittestchildren="false">
			<CSGOHudWeaponSelection id="HudWeaponSelection"/>
			<CSGOHudWeaponPanel id="HudWeaponPanel"/>
		</Panel>

		                                                                
		<CSGOHudFreezePanel id="HudFreezePanel" hittest="false" hittestchildren="false" />

		<CSGOHudBlurTarget id="HudBlur" blurrects="Scoreboard EndOfMatch BuyMenu" class="HudBlur" hittest="false" hittestchildren="false">
			<CSGOBackbufferImagePanel class="full-width full-height" />
		</CSGOHudBlurTarget>

		<CSGOHudSpectator id="HudSpectator" />
		<CSGOHudDemoPlayback id="HudDemoPlayback" />

		<CSGOHudRosettaSelector id="HudRosettaSelector" />
		<CSGOMapOverview id="MapOverview"/>

		<CSGOBuyMenu id="BuyMenu" />
		<CSGOScoreboard id="Scoreboard" />
		<CSGOEndOfMatch id="EndOfMatch" />

		<CSGOTeamSelectMenu id="TeamSelectMenu"/>

		            
		                                                   
		                                           
		            
		
		                                                       
		<CSGOHudChat id="HudChat"/>
		<CSGOHudVote id="HudVote"/>
		
		                                                                   
		<Panel id="ItemPreviewDebugManager" hittest="false" />
		<CSGOPopupManager id="PopupManager" onpopupsdismissed="" />
		<ContextMenuManager id="ContextMenuManager" />
		<CSGOTooltipManager id="TooltipManager" hittest="false" />

	</CSGOHud>
</root>
